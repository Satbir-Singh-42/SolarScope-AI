# Development Environment Variables
# Copy this file to .env and fill in your values

# Required for AI analysis
GOOGLE_API_KEY=your_google_ai_api_key_here

# Optional - Database connection (if not set, uses memory storage)
# DATABASE_URL=postgresql://user:password@localhost:5432/solarscope

# Optional - Session secret (used for cookie signing)
# SESSION_SECRET=your_random_secret_here

# Development settings (automatically set by npm scripts)
NODE_ENV=development
PORT=5000